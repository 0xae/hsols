{% extends 'BackendBundle:Default:layout.html.twig' %}

{% block content %}

<div class="row">
    <div class="col-xs-12">
        <div class="box">
            <div class="box-header">
                <h3 class="box-title">CompBook #{{ entity.id }}</h3>
                <p>
                    <a href="{{ path('administration_CompBook') }}">
                        <span class="glyphicon glyphicon-back"></span> Voltar
                    </a>
                </p>
            </div>
            <!-- /.box-header -->

            <div class="box-body no-padding">
                <table class="table table-hover table-striped">
                    <tbody>
                        <tr>
                            <th>Id</th>
                            <td>{{ entity.id }}</td>
                        </tr>
                        <tr>
                            <th>Clientname</th>
                            <td>{{ entity.clientName }}</td>
                        </tr>
                        <tr>
                            <th>Suppliername</th>
                            <td>{{ entity.supplierName }}</td>
                        </tr>
                        <tr>
                            <th>Supplieraddress</th>
                            <td>{{ entity.supplierAddress }}</td>
                        </tr>
                        <tr>
                            <th>Clientaddress</th>
                            <td>{{ entity.clientAddress }}</td>
                        </tr>
                        <tr>
                            <th>Clientnacionality</th>
                            <td>{{ entity.clientNacionality }}</td>
                        </tr>
                        <tr>
                            <th>Clientphone</th>
                            <td>{{ entity.clientPhone }}</td>
                        </tr>
                        <tr>
                            <th>Clientpassport</th>
                            <td>{{ entity.clientPassport }}</td>
                        </tr>
                        <tr>
                            <th>Clientemail</th>
                            <td>{{ entity.clientEmail }}</td>
                        </tr>
                        <tr>
                            <th>Complaint</th>
                            <td>{{ entity.complaint }}</td>
                        </tr>
                        <tr>
                            <th>Complaintdate</th>
                            <td>{{ entity.complaintDate|date('Y-m-d H:i:s') }}</td>
                        </tr>
                        <tr>
                            <th>Createdat</th>
                            <td>{{ entity.createdAt|date('Y-m-d H:i:s') }}</td>
                        </tr>
                    </tbody>
                </table>
            </div>
            <!-- /.box-body -->
        </div>
        <!-- /.box -->
    </div>
</div>

{% endblock %}

